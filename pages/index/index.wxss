page {
  height: 100%;
}
.header{
  border-bottom: 1px solid #ccc;
}
.float-icon{
  vertical-align: middle;
}
.container{
  padding: 5rpx;
  margin: 10rpx;
  height: 95%;
  overflow: auto;
}
.code-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.code-row{
  white-space:nowrap;
}
.code-text{
  display:inline-block;
  /* font-size: 16rpx; */
  /* line-height: 20rpx; */
  /* height: 20rpx; */
}
.sidebar{
  height: 100%;
  overflow: auto;
  position: absolute;
  top: 0;
  left: -600rpx;
  max-width: 600rpx;
  min-width: 400rpx;
  padding-right: 10rpx;
  border-right:1px solid #ccc;
  border-top:1px solid #ccc;
  height: 100%;
  background: #fff;
}
.sidebar-title {
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  align-items:center;
}
.sidebar-title-name {
  max-width: 400rpx;
  padding-left: 10rpx;
  font-size:  12px;
  word-break: break-all;
}
.sidebar-title-branch{
  color: #0366d6;
}
.sidebar-main{
}
.sidebar-icon{
  width: 50rpx;
  height: 50rpx;
}
.loading {
  justify-content:center;
  align-items:center;
  position: absolute;
  right:0;
  top: 0;
  left: 0;
  bottom: 0;
  display: none;
}
.loading-img{
  width: 50px;
  height: 50px;
}
.show {
  display:flex;
}