.header{
}
.float-icon{
  vertical-align:bottom;
}
.container {
  height: 95%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  border: 1px solid #ccc;
  padding: 5rpx;
  margin: 10rpx;
}
.code-row{
  white-space:nowrap;
}
.code-text{
  display:inline-block;
  /* font-size: 16rpx; */
  /* line-height: 20rpx; */
  /* height: 20rpx; */
}
.sidebar{
  position: absolute;
  top: 0;
  left: -600rpx;
  max-width: 600rpx;
  min-width: 400rpx;
  padding-right: 10rpx;
  border-right:1px solid #ccc;
  border-top:1px solid #ccc;
  height: 100%;
  background: #fff;
}
.sidebar-title {
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  align-items:center;
}
.sidebar-title-name {
  max-width: 400rpx;
  padding-left: 10rpx;
  font-size:  12px;
  word-break: break-all;
}
.sidebar-title-branch{
  color: #0366d6;
}
